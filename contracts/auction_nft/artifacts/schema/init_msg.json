{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "auction_blocks",
    "denom",
    "fee",
    "name",
    "step_price"
  ],
  "properties": {
    "auction_blocks": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "denom": {
      "type": "string"
    },
    "fee": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "name": {
      "type": "string"
    },
    "step_price": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
